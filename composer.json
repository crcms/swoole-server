{
  "name": "crcms/swoole-server",
  "description": "The CrCms Swoole Server.",
  "keywords": [
    "swoole",
    "http",
    "tcp",
    "laravel"
  ],
  "homepage": "http://blog.crcms.cn",
  "support": {
    "issues": "https://github.com/crcms/swoole-server/issues",
    "source": "https://github.com/crcms/swoole-server"
  },
  "authors": [
    {
      "name": "simon",
      "email": "simon@crcms.cn"
    }
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.2.4",
    "ext-json": "*",
    "ext-swoole": "^4.2",
    "illuminate/config": "^5.7",
    "illuminate/console": "^5.7",
    "illuminate/container": "^5.7",
    "illuminate/events": "^5.7",
    "illuminate/http": "^5.7",
    "illuminate/pipeline": "^5.7",
    "illuminate/redis": "^5.7",
    "illuminate/support": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "codedungeon/phpunit-result-printer": "^0.26.0",
    "mockery/mockery": "^1.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/php-code-coverage": "^6.0",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "CrCms\\Server\\": "src/"
    },
    "files": [
      "src/helpers.php",
      "src/Drivers/Laravel/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\Server\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "CrCms\\Server\\Drivers\\Laravel\\SwooleServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
